#!/usr/bin/perl

# PODNAME: Tapper API


use strict;
use warnings;

BEGIN {
    use CGI::Carp qw(carpout);
    open( my $fh_log, '>>', '/tmp/tapper-rest-api.log')
        or die "Unable to open log for tapper-rest-api: $!\n"
    ;
    carpout( $fh_log );
}

use Mojolicious::Commands;

Mojolicious::Commands->start_app('Tapper::API');

